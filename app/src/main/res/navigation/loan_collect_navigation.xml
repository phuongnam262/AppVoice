<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/loan_collect_navigation"
    app:startDestination="@id/recognizeFaceFragment">
    <fragment
        android:id="@+id/recognizeFaceFragment"
        android:name="com.lock.smartlocker.ui.recognize_face.RecognizeFaceFragment"
        android:label="fragment_recognize_face"
        tools:layout="@layout/fragment_recognize_face" >
        <action
            android:id="@+id/action_recognizeFaceFragment_to_inputOTPFragment2"
            app:destination="@id/inputOTPFragment2" />
    </fragment>
    <fragment
        android:id="@+id/inputOTPFragment2"
        android:name="com.lock.smartlocker.ui.inputotp.InputOTPFragment"
        android:label="fragment_input_otp"
        tools:layout="@layout/fragment_input_otp" >
        <action
            android:id="@+id/action_inputOTPFragment2_to_categoryFragment"
            app:destination="@id/categoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/thankFragment2"
        android:name="com.lock.smartlocker.ui.thank.ThankFragment"
        android:label="fragment_thank"
        tools:layout="@layout/fragment_thank" />
    <fragment
        android:id="@+id/categoryFragment"
        android:name="com.lock.smartlocker.ui.category.CategoryFragment"
        android:label="fragment_category"
        tools:layout="@layout/fragment_category" >
        <action
            android:id="@+id/action_categoryFragment_to_cartFragment"
            app:destination="@id/cartFragment" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.lock.smartlocker.ui.cart.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_cartFragment_to_collectItemFragment"
            app:destination="@id/collectItemFragment" />
    </fragment>
    <fragment
        android:id="@+id/collectItemFragment"
        android:name="com.lock.smartlocker.ui.collect_items.CollectItemFragment"
        android:label="fragment_collect_item"
        tools:layout="@layout/fragment_collect_item" />
</navigation>