<?xml version="1.0" encoding="utf-8"?>
<com.lock.smartlocker.util.view.custom.CustomLoading xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/custom_loading"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/loading_1"
        android:layout_width="@dimen/dp_25"
        android:layout_height="@dimen/dp_25"
        android:layout_gravity="center" />

    <ImageView
        android:id="@+id/loading_2"
        android:layout_width="@dimen/dp_25"
        android:layout_marginHorizontal="@dimen/dp_4"
        android:layout_height="@dimen/dp_25"
        android:layout_gravity="center"/>

    <ImageView
        android:id="@+id/loading_3"
        android:layout_width="@dimen/dp_25"
        android:layout_height="@dimen/dp_25"
        android:layout_gravity="center" />
</com.lock.smartlocker.util.view.custom.CustomLoading>